<h1 class="title is-3">Search Queries</h1>

<%= link_to "Create new", new_search_query_path, class: 'button' %>

<h2 class="title is-5">Existing Search Queries</h2>

<ul class="list is-marginless">
  <% @search_queries.each do |sq| %>
    <li class="list-item">
      <%= link_to "#{sq.name} (#{sq.url})", sq %>
    </li>
  <% end %>
</ul>

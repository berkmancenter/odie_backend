<h1 class="title is-3">Cohorts</h1>

<%= link_to "Create manually", new_cohort_path, class: 'button' %>

<h2 class="title is-5">Existing Cohorts</h2>

<ul class="list is-marginless">
  <% Cohort.all.each do |cohort| %>
    <li class="list-item">
      <%= cohort.id %>. <%= link_to cohort.description, cohort %>
    </li>
  <% end %>
</ul>
